package funcoes;
import java.util.Scanner;
	
public class Soma {
		
	public double a,b;
	
	public double getSoma() {
		
		return a + b;
	}

	public double getA() {
		return a;
	}

	public void setA(double a) {
		this.a = a;
	}

	public double getB() {
		return b;
	}

	public void setB(double b) {
		this.b = b;
	}
}
